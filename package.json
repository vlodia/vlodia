{
  "name": "vlodia",
  "version": "0.1.1",
  "description": "A next-generation TypeScript ORM with reflection, type safety, and database-agnostic design",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vlodia": "dist/cli/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch --preserveWatchOutput",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "vlodia": "node dist/cli/index.js",
    "start": "node dist/index.js",
    "dev:cli": "tsc && node dist/cli/index.js",
    "benchmark": "node dist/benchmarks/index.js",
    "docs": "typedoc src/index.ts",
    "docs:serve": "typedoc --serve src/index.ts",
    "check": "npm run lint && npm run format:check && npm run test",
    "release": "npm run build && npm publish",
    "release:beta": "npm run build && npm publish --tag beta",
    "release:alpha": "npm run build && npm publish --tag alpha"
  },
  "keywords": [
    "orm",
    "typescript",
    "database",
    "postgresql",
    "mysql",
    "sqlite",
    "reflection",
    "decorators",
    "type-safe",
    "migrations",
    "caching",
    "transactions",
    "repository-pattern",
    "query-builder",
    "entity-framework",
    "active-record",
    "data-mapper",
    "unit-of-work",
    "identity-map"
  ],
  "author": "Vlodia ORM Team",
  "license": "MIT",
  "homepage": "https://github.com/vlodia/vlodia",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlodia/vlodia.git"
  },
  "bugs": {
    "url": "https://github.com/vlodia/vlodia/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "ora": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "sqlite": "^5.1.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/mysql": "^2.15.27",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "benchmark": "^2.1.4",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "ora": "^7.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "pg": "^8.11.0",
    "redis": "^4.6.10"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "mysql": {
      "optional": true
    },
    "sqlite": {
      "optional": true
    },
    "redis": {
      "optional": true
    }
  }
}
