{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "name": "Vlodia ORM",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "theme": "default",
  "readme": "README.md",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Decorators",
    "Adapters",
    "Repository",
    "Validation",
    "Caching",
    "Migrations",
    "CLI",
    "Utils",
    "Examples",
    "Other"
  ],
  "sort": ["source-order"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "gitRevision": "main",
  "gitRemote": "origin",
  "sourceLinkTemplate": "https://github.com/vlodia/vlodia/blob/{gitRevision}/{path}#L{line}"
}
